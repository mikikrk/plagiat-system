<?xml version="1.0"?><!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.zpi.plagiarism_detector.server.database">
    <class name="com.zpi.plagiarism_detector.server.database.ArticleLink" table="ARTICLE_LINKS">
        <id name="articleId" column="ARTICLE_ID">
            <generator class="native" />
        </id>

        <property name="articleAddress" column="ADDRESS" type="string" length="255"
                  unique="true" not-null="true">
        </property>


    </class>
</hibernate-mapping>