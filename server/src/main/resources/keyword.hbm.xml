<?xml version="1.0"?><!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.zpi.plagiarism_detector.server.database">
    <class name="com.zpi.plagiarism_detector.server.database.Keyword" table="KEYWORD">

        <id name="id" column="ID">
            <generator class="native"/>
        </id>
        <property name="word" column="WORD" type="string" length="200" unique="true">
        </property>

    </class>
</hibernate-mapping>